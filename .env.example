# AI Interview Assistant Configuration
REACT_APP_NAME=AI Interview Assistant
REACT_APP_VERSION=1.0.0
REACT_APP_ENVIRONMENT=development

# AI Service Configuration
# Add your OpenAI API key to enable real AI-powered interviews
# Without this, the app will use curated questions and rule-based evaluation
REACT_APP_OPENAI_API_KEY=your_openai_api_key_here
REACT_APP_AI_MODEL=gpt-3.5-turbo
REACT_APP_ENABLE_AI=true

# Interview Configuration
REACT_APP_MAX_QUESTIONS=6
REACT_APP_EASY_TIME_LIMIT=20
REACT_APP_MEDIUM_TIME_LIMIT=60
REACT_APP_HARD_TIME_LIMIT=120

# UI Configuration
REACT_APP_THEME_PRIMARY_COLOR=#1890ff
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_EXPORT=true